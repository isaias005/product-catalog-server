{
  "kind": "collectionType",
  "collectionName": "productos",
  "info": {
    "name": "Producto"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "titulo": {
      "type": "string"
    },
    "descripcion": {
      "type": "text",
      "maxLength": 1000,
      "minLength": 6
    },
    "imagen": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    }
  }
}
